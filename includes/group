  </span><span class="s2">"group_id"</span><span class="p">:</span><span class="w"> </span><span class="s2">"ccf60309-2f0a-40e6-b348-9021b1b57fa6"</span><span class="p">,</span><span class="w">
  </span><span class="s2">"users"</span><span class="p">:</span><span class="w"> </span><span class="p">[</span><span class="w">
    </span><span class="s2">"46406186-f4e5-4a52-8bba-3668d1fc338f"</span><span class="p">,</span><span class="w">
    </span><span class="s2">"658384cb-a67a-470e-a4ba-5a16098d7589"</span><span class="w">
  </span><span class="p">]</span><span class="w">
</span><span class="p">}</span><span class="w">
